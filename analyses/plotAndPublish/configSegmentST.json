    "Phase-1 phi n Segments"   :
    {
        "inputs"    :
        {
            "nsegments"   :
            {
                "label"             : "nseg",
                "legendEntry"       : "nseg",
                "folder"            : "",
                "plot"		    : "hNsegment"
	    }
	    
        },
        "plot"      :
        {
            "x"                 : [20.0, 20.0, "# segments"],
            "y"                 : [0.5, 12.5, "entries"],
            "option"            : "hist",
            "legendTitle"       : "Phase-1 n segments",
            "caption"           : "DT SliceTest",
            "logo"              : ["CMS", "Preliminary"],
            "colorMap"          : [1, 2, 3, 4, 5, 6, 7, 8, 9],
            "markerMap"         : [20, 21, 22, 23, 24, 25, 26],
            "legendRange"       : [0.49, 0.67, 0.85, 0.80]
        },
        "output"    :
        {
            "filenamePlot"      : "Phase1_nSegmens",
            "fileType"          : ["png", "pdf"]
        }
    },

